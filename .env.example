# Garage S3 Configuration
# Copy this file to .env and fill in your Garage HQ server details

# Garage endpoint (hostname only, e.g., s3.garage.example.com or localhost:3900)
# Do NOT include the scheme (http:// or https://)
# The scheme is controlled by the 'insecure' parameter in the service configs
GARAGE_ENDPOINT=

# Garage access credentials
GARAGE_ACCESS_KEY_ID=
GARAGE_SECRET_ACCESS_KEY=

# Loki & Mimir tenant id
TENANT_ID=